{
  "name": "linter-govet",
  "linter-package": true,
  "main": "./lib/linter-govet",
  "version": "1.0.0",
  "private": true,
  "description": "Check Go files with go vet tool",
  "keywords": [
    "go",
    "golang",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-govet.git"
  },
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-govet/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-govet",
  "license": "MIT",
  "engines": {
    "atom": ">=1.9.0 <2.0.0"
  },
  "configSchema": {
    "executablePath": {
      "title": "Go Executable Path",
      "type": "string",
      "description": "The path to the `go` executable. Find by running `which go`",
      "default": "go"
    },
    "verboseMode": {
      "type": "boolean",
      "default": true,
      "description": "Verbose mode for `go vet` command"
    },
    "extraOptions": {
      "type": "string",
      "default": "",
      "description": "Options for `go vet` command"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "atom --test spec"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "6.0.0"
  },
  "devDependencies": {
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "jasmine-fix": "1.3.1"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
